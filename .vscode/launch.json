  "version": "0.2.0",
  "configurations": [
    {
      "name": "启动",
      "type": "node",
      "request": "launch",
      "restart": true,
      "sourceMaps": true,
      "runtimeArgs": [
        "-r",
        "${workspaceFolder}/node_modules/ts-node/register",
        "-r",
        "${workspaceFolder}/node_modules/tsconfig-paths/register"
      ],
      "program": "${workspaceFolder}/src/server.ts",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
    }
  ]
}
